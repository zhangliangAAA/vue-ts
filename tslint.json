{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "double"
    ],
    "semicolon": [true, "always", "ignore-interfaces"], //分号的使用规则
    "indent": [
      true
    ],
    "interface-name": [
      false
    ],
    "arrow-parens": false,
    // Pending fix for shorthand property names.
    "object-literal-sort-keys": false,
    "no-unused-expression": false
  },
  "rulesDirectory": [],
  // "enable": true,
  // "jsEnable": true,
  "autoFixOnSave": true
}
